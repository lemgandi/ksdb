####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)$QTDIR/plugins/sqldrivers/" || $(MKDIR) "$(INSTALL_ROOT)$QTDIR/plugins/sqldrivers/"
	-$(INSTALL_FILE) "../sqldrivers/$(TARGET)" "$(INSTALL_ROOT)$QTDIR/plugins/sqldrivers/$(TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)$QTDIR/plugins/sqldrivers/$(TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)$QTDIR/plugins/sqldrivers/"


install: install_target  

uninstall: uninstall_target  

