#############################################################################
# Makefile for building: sqlite3
# Generated by qmake (1.07a) (Qt 3.3.8b) on: Thu Dec 31 11:17:56 2009
# Project:  sqlite3.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile sqlite3.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-sqlite3

first: all

all: Makefile $(SUBTARGETS)

sqlite3/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "sqlite3" || $(MKDIR) "sqlite3"
	cd sqlite3 && $(QMAKE) sqlite3.pro -o $(MAKEFILE)
sub-sqlite3: sqlite3/$(MAKEFILE) FORCE
	cd sqlite3 && $(MAKE) -f $(MAKEFILE)

Makefile: sqlite3.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile sqlite3.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile sqlite3.pro

all: $(SUBTARGETS)
qmake_all: sqlite3/$(MAKEFILE)
	 ( [ -d sqlite3 ] && cd sqlite3 ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d sqlite3 ] && cd sqlite3 ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d sqlite3 ] && cd sqlite3 ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d sqlite3 ] && cd sqlite3 ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d sqlite3 ] && cd sqlite3 ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

